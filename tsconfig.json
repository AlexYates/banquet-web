{
    "compilerOptions": {
        "baseUrl": ".",
        "composite": true,
        "noEmit": true,
        "paths": {
            "@/*": [
                "./src/*"
            ]
        },
        "types": [
            "vite/client"
        ]
    },
    "exclude": [
        "src/**/__tests__/*"
    ],
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": [
        "env.d.ts",
        "src/**/*",
        "src/**/*.vue"
    ]
}